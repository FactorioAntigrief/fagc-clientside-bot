  "compilerOptions": {
    "outDir": "./dist",
    "allowJs": true,
    "target": "es6",
	"moduleResolution": "node",
	"module": "commonjs",
	"allowSyntheticDefaultImports": true,
	"esModuleInterop": true,
	"resolveJsonModule": true
  },
  "include": ["./src/**/*"],
  "exclude": ["node_modules"],
}